# SDSC CA Policy

access_id_CA   X509    '/C=US/O=SDSC/OU=SDSC-CA/CN=Certificate Authority/UID=certman'
pos_rights     globus  CA:sign
cond_subjects  globus  '/C=US/O=SDSC/*'

access_id_CA   X509    '/C=US/O=SDSC/OU=SDSC-CA/CN=Certificate Authority/USERID=certman'
pos_rights     globus  CA:sign
cond_subjects  globus  '/C=US/O=SDSC/*'
